class Solution(object):
    def climbStairs(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n <= 1:
            return 1
        f = []
        f.append(1)
        f.append(2)
        for i in range (2, n):
            # 2 to n coz for 0 and 1th step it will take 1 way to reach to the top and so will            
            # start from 2 to nth step 
            f.append(f[n-1] + f[n-2])
        return f[n-1]
